- for (var i = 0; i < users.length; ++i) { 
    <tr>
    <td>#{i+1}</td>
    <td>#{users[i].name}</td>
    <td>#{users[i].email}</td>
    <td>#{users[i].gender}</td>
    <td>#{users[i].status}</td>
    <td>
        <a href="/update" class="btn update"><span class="action_btn"><i class="fas fa-pencil-alt"></i></span></a>
        <a href="/delete" class="btn delete"><span class="action_btn"><i class="fas fa-trash"></i></span></a>
    </td>
    </tr>
    - }